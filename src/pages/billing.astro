---
export const prerender = false;
import Base from '../layouts/Base.astro';
const user = Astro.locals.user;
if (!user) return Astro.redirect('/dashboard');
---
<Base title="Billing">
  <h1 class="text-3xl font-semibold text-ink-100 mb-2">Billing</h1>
  <p class="text-ink-400 mb-8">Your payment history and receipts.</p>
  <div class="text-ink-500 text-sm">Billing history coming soon.</div>
</Base>
